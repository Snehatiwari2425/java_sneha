<?xml version="1.0" encoding="UTF-8"?>

<beans   xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" >
               
       <bean id="q" class="com.depend.collection.inject.Ques">    
            <constructor-arg value="what is spring?" ></constructor-arg>   
            <constructor-arg>
            
            <list>
            <value>spring is the most popular java framework</value>
            <value>spring framework makes application easier.</value>
            <value>spring is the framework of frameworks.</value>
            </list>
            
            </constructor-arg>
            </bean>
 </beans>